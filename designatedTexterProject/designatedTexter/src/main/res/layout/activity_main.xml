<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    tools:ignore="MergeRootFrame"
    android:clickable="true"
    android:background="@drawable/background">

    <EditText
        android:layout_width="274dp"
        android:maxLines="1"
        android:layout_height="80dp"
        android:id="@+id/destLoc"
        android:layout_gravity="center"
        android:layout_marginRight="2dp"
        android:layout_x="52dp"
        android:layout_y="289dp" />

    <Button
        android:layout_width="112dp"
        android:layout_height="wrap_content"
        android:id="@+id/button"
        android:layout_gravity="right|center_vertical"
        android:layout_marginRight="-4dp"
        android:text="Send"
        android:layout_marginLeft="-376dp"
        android:layout_marginBottom="-37dp"
        android:layout_x="254dp"
        android:layout_y="457dp" />

    <EditText
        android:layout_width="274dp"
        android:maxLines="1"
        android:layout_height="80dp"
        android:id="@+id/myLoc"
        android:layout_x="55dp"
        android:layout_y="207dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Destination"
        android:id="@+id/textView"
        android:layout_x="162dp"
        android:layout_y="310dp"
        android:fontFamily="sans-serif-condensed"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="My Location"
        android:id="@+id/textView2"
        android:layout_x="162dp"
        android:layout_y="225dp"
        android:fontFamily="sans-serif-condensed"/>


    <RadioGroup
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="30dp"
        android:layout_y="387dp">

        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/busButton"
            android:layout_gravity="center_horizontal"
            android:checked="false" />

        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/driveButton"
            android:layout_x="33dp"
            android:layout_y="381dp"
            android:layout_gravity="center_horizontal"
            android:checked="true" />

        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/cycleButton"
            android:layout_x="225dp"
            android:layout_y="7dp"
            android:layout_gravity="center_horizontal" />

        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/walkButton"
            android:checked="false"
            android:layout_x="152dp"
            android:layout_y="0dp"
            android:layout_gravity="center_horizontal" />

    </RadioGroup>

    <ImageView
        android:layout_width="23dp"
        android:layout_height="24dp"
        android:id="@+id/imageView"
        android:layout_x="90dp"
        android:layout_y="403dp" />

    <ImageView
        android:layout_width="23dp"
        android:layout_height="24dp"
        android:id="@+id/imageView2"
        android:layout_x="97dp"
        android:layout_y="442dp" />

    <ImageView
        android:layout_width="23dp"
        android:layout_height="24dp"
        android:id="@+id/imageView3"
        android:layout_x="7dp"
        android:layout_y="389dp"
        android:background="@drawable/bus" />

    <ImageView
        android:layout_width="23dp"
        android:layout_height="24dp"
        android:id="@+id/imageView4"
        android:layout_x="9dp"
        android:layout_y="421dp"
        android:baselineAlignBottom="true"
        android:background="@drawable/car" />

    <ImageView
        android:layout_width="23dp"
        android:layout_height="24dp"
        android:id="@+id/imageView5"
        android:layout_x="9dp"
        android:layout_y="453dp"
        android:background="@drawable/cycle" />

    <ImageView
        android:layout_width="23dp"
        android:layout_height="24dp"
        android:id="@+id/imageView6"
        android:layout_x="9dp"
        android:layout_y="485dp"
        android:background="@drawable/walk" /><![CDATA[



















    android:src="@drawable-hdpi/ic_launcher.png" />
        android:background="@drawable-hdpi/ic_launcher.png" />









]]>
</AbsoluteLayout>
