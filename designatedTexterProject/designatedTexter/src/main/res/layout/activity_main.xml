<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    tools:ignore="MergeRootFrame"
    android:clickable="true"
    android:background="@drawable/background">

    <EditText
        android:layout_width="274dp"
        android:maxLines="1"
        android:layout_height="80dp"
        android:id="@+id/destLoc"
        android:layout_gravity="center"
        android:layout_x="52dp"
        android:layout_y="289dp"
        android:layout_above="@+id/radioGroup"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="29dp" />

    <EditText
        android:layout_width="274dp"
        android:maxLines="1"
        android:layout_height="80dp"
        android:id="@+id/myLoc"
        android:layout_x="55dp"
        android:layout_y="207dp"
        android:layout_marginBottom="111dp"
        android:layout_above="@+id/radioGroup"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Destination"
        android:id="@+id/textView"
        android:layout_x="162dp"
        android:layout_y="310dp"
        android:fontFamily="sans-serif-condensed"
        android:layout_alignTop="@+id/destLoc"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="My Location"
        android:id="@+id/textView2"
        android:layout_x="162dp"
        android:layout_y="225dp"
        android:fontFamily="sans-serif-condensed"
        android:layout_alignTop="@+id/myLoc"
        android:layout_centerHorizontal="true" />


    <RadioGroup
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="30dp"
        android:layout_y="387dp"
        android:layout_alignTop="@+id/linearLayout"
        android:layout_toRightOf="@+id/linearLayout"
        android:id="@+id/radioGroup">

        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/busButton"
            android:layout_gravity="center_horizontal"
            android:checked="false" />

        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/driveButton"
            android:layout_x="33dp"
            android:layout_y="381dp"
            android:layout_gravity="center_horizontal"
            android:checked="true" />

        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/cycleButton"
            android:layout_x="225dp"
            android:layout_y="7dp"
            android:layout_gravity="center_horizontal" />

        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/walkButton"
            android:checked="false"
            android:layout_x="152dp"
            android:layout_y="0dp"
            android:layout_gravity="center_horizontal" />

    </RadioGroup>

    <ImageView
        android:layout_width="23dp"
        android:layout_height="24dp"
        android:id="@+id/imageView"
        android:layout_x="90dp"
        android:layout_y="403dp" />

    <ImageView
        android:layout_width="23dp"
        android:layout_height="24dp"
        android:id="@+id/imageView2"
        android:layout_x="97dp"
        android:layout_y="442dp"
        android:layout_below="@+id/imageView"
        android:layout_toRightOf="@+id/linearLayout" />

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="42dp"
        android:layout_height="wrap_content"
        android:layout_x="0dp"
        android:layout_y="387dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:id="@+id/linearLayout">

        <ImageView
            android:layout_width="33dp"
            android:layout_height="33dp"
            android:id="@+id/imageView3"
            android:layout_x="122dp"
            android:layout_y="464dp"
            android:background="@drawable/bus" />

        <ImageView
            android:layout_width="33dp"
            android:layout_height="33dp"
            android:id="@+id/imageView4"
            android:layout_x="145dp"
            android:layout_y="383dp"
            android:baselineAlignBottom="true"
            android:background="@drawable/car" />

        <ImageView
            android:layout_width="33dp"
            android:layout_height="33dp"
            android:id="@+id/imageView5"
            android:layout_x="194dp"
            android:layout_y="385dp"
            android:background="@drawable/cycle" />

        <ImageView
            android:layout_width="33dp"
            android:layout_height="33dp"
            android:id="@+id/imageView6"
            android:layout_x="189dp"
            android:layout_y="464dp"
            android:background="@drawable/walk" />
    </LinearLayout>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button"
        android:background="@android:drawable/ic_menu_send"
        android:layout_alignBottom="@+id/radioGroup"
        android:layout_alignParentRight="true"
        android:layout_toRightOf="@+id/destLoc" /><![CDATA[





























    android:src="@drawable-hdpi/ic_launcher.png" />
        android:background="@drawable-hdpi/ic_launcher.png" />














]]>
</RelativeLayout>
